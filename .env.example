# Ethereum RPC endpoint URL (e.g. Infura, Alchemy, or your own node)
RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key

# Private key of the initial wallet with funds (without 0x prefix)
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Chain ID (1 for mainnet, 5 for goerli, etc.)
CHAIN_ID=1

# Number of addresses to activate
ADDRESS_COUNT=5 